<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.sshadd</string>

    <key>OnDemand</key>
    <false/>

    <key>UserName</key>
    <string>@USER</string>

    <key>GroupName</key>
    <string>users</string>

    <key>ProgramArguments</key>
    <array>
            <string>/usr/bin/ssh-add</string>
            <string>-K</string>
    </array>
</dict>
</plist>
