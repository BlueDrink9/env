{
    // Don't autoselect first completion item.
    "suggest.noselect": true,
    "suggest.echodocSupport": true,
    // "diagnostic.displayByAle": true,
    "diagnostic.virtualText": false,
    "diagnostic.errorSign": "X",
    "diagnostic.warningSign": "!",
    // idk what the differences are here.
    "diagnostic.infoSign": "i",
    "diagnostic.hintSign": "?",
    "codeLens.enable": true,
    // "list.previewHighlightGroup": "Search",
    "coc.preferences.extensionUpdateCheck": "weekly",
    // Maybe.
    "coc.preferences.enableFloatHighlight": false,
    // Priority for signcolumn goes to diagnostics.
    "diagnostic.signOffset": 9999,
    // Use microsoft python lsp instead of Jedi.
    "python.jediEnabled": true,
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/10.0.0/clangd_10.0.0/bin/clangd",
    // Faster completion at the cost of goto definition.
    "jedi.jediSettings.autoImportModules": ["numpy", "pandas"],
    "languageserver": {
        "openscad-dhzu": {
            "command": "openscad-language-server",
            "args": [""],
            "filetypes": ["openscad"],
            "settings": {
            },
        },
        "openscad-Leathong": {
            // Binary is openscad-lsp
            "command": "openscad-lsp",
            "args": ["--stdio"],
            "filetypes": ["openscad"],
        },
    },
	  "suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"constructor": "\uf0ad",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
    "snippets.textmateSnippetsRoots": [
        "~/.vim/plugins/vscode-openscad/snippets"
    ],
    }
