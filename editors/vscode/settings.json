{
    // Temporarily enable this only, when needing to install extensions if ssl issues crop up.
    // "http.proxyStrictSSL": false,
    "http.experimental.systemCertificatesV2": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.selectionMode": "never",
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorSurroundingLines": 3,
    "editor.definitionLinkOpensInPeek": true,
    "editor.fontFamily": "'SauceCodePro NF', 'Source Code Pro', Consolas, 'Courier New', monospace",
    "editor.lineNumbers": "relative",
    "editor.linkedEditing": true,
    "editor.renderWhitespace": "trailing",
    "editor.smoothScrolling": true,
    "editor.suggest.insertMode": "replace",
    "editor.wordWrap": "on",
    "editor.rulers": [80, 120],
    "editor.minimap.renderCharacters": false,

    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
        "vscodevim.vim": 1
    },

    "files.autoGuessEncoding": true,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,

    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableCommitSigning": false,
    "git.enableSmartCommit": true,

    "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",

    "security.workspace.trust.untrustedFiles": "open",

    "telemetry.telemetryLevel": "off",

    "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",

    "emmet.showSuggestionsAsSnippets": true,

    "mssql.enableQueryHistoryCapture": true,

    "poor-mans-t-sql-formatter-pg.std.breakJoinOnSections": true,
    "poor-mans-t-sql-formatter-pg.std.expandBetweenConditions": false,
    "poor-mans-t-sql-formatter-pg.std.expandInLists": true,
    "poor-mans-t-sql-formatter-pg.std.indent": "4",
    "poor-mans-t-sql-formatter-pg.std.keywordStandardization": true,
    "poor-mans-t-sql-formatter-pg.std.maxLineWidth": 80,
    "poor-mans-t-sql-formatter-pg.std.spaceAfterExpandedComma": true,
    "poor-mans-t-sql-formatter-pg.std.trailingCommas": false,

    "prettier-sql.commaPosition": "before",
    "prettier-sql.keywordCase": "upper",
    "prettier-sql.linesBetweenQueries": 2,
    "prettier-sql.logicalOperatorNewline": "after",

    "vscode-neovim.revealCursorScrollLine": true,
    "vscode-neovim.compositeKeys": {
        "kv": {
            "command": "vscode-neovim.escape"
        },
        "vk": {
            "command": "vscode-neovim.escape"
        },
    },

    "workbench.colorTheme": "Ayu Light Bordered",

    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.highlightedyank.enable": true,
    "vim.leader": "<space>",
    "vim.showMarksInGutter": true,
    "vim.smartRelativeLine": true,
    "vim.sneak": false,  // replaced with leap extension
    "vim.targets.enable": true,
    "vim.vimrc.enable": true,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["k", "v"],
            "after": ["<Esc>"]
        },
        {
            "before": ["v", "k"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<c-e>"],
            "commands": ["editor.action.insertSnippet"],
        },
        {
            "before": ["<c-b>"],
            "commands": ["cquery.snippetPrev"],
        },
        {
            "before": ["<c-f>"],
            "commands": ["cquery.snippetNext"],
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                ";"
            ],
            "commands": [
                "vim.showQuickpickCmdLine"
            ]
        },
        {
            "before": [ "<leader>", "c" ],
            "after": [ "g", "c" ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        },
        {
            "before": ["Y"],
            "after": ["y", "$"]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                ","
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "a"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [ "\"", "\"" ],
            "after": [ "\"", "*" ]
        },
        {
            "before": [ "d", "r" ],
            "after": [ "\"", "_", "d" ]
        },
        // {
        //     "before": [
        //         "<leader>",
        //         "s",
        //         "s"
        //     ],
        //     "commands": [
        //         "jupyter.execSelectionInteractive"
        //     ],
        // },
        // {
        //     "before": [
        //         "<leader>",
        //         "shift+s"
        //     ],
        //     "commands": [
        //         "jupyter.runFileInteractive"
        //     ],
        // },
        {
            "before": [
                "s"
            ],
            "commands": [
                "leap.find"
            ]
        },
        {
            "before": [
                "right"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "left"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": ["<leader>", "i", "h"],
            "commands": ["editor.action.showHover"]
        },
        {
            "before": ["<leader>","i","r","r"],
            "commands": ["editor.action.refactor"]
        },
        {
            "before": ["<leader>", "i", "r", "n"],
            "commands": ["editor.action.rename"]
        },
        {
            "before": ["<leader>", "i", "?"],
            "commands": ["editor.action.showHover"]
        },
        {
            "before": ["<leader>", "e", "f"],
            "commands": ["editor.action.autoFix"]
        },
        {
            "before": ["<leader>", "i", "a"],
            "commands": ["editor.action.quickFix"]
        },
        {
            "before": ["g", "r"],
            "commands": ["editor.action.findReferences"]
        },
        {
            "before": ["<leader>", "g", "h"],
            "commands": ["editor.action.goToDeclaration"]
        },
        {
            "before": ["<leader>", "g", "H"],
            "commands": ["editor.action.peakDefinition"]
        },
        {
            "before": ["g", "i"],
            "commands": ["editor.gotoImplementation"],
        },
        {
            "before": ["<leader>", "i", "f"],
            "commands": ["editor.action.findReferences"]
        },
        {
            "before": ["g", "r"],
            "commands": ["editor.action.referenceSearch.trigger"],
        },
        {
            "before": ["<leader>","<leader>"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", ","],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", "i", "T"],
            "commands": ["workbench.action.showAllSymbols"]
        },
        {
            "before": ["<leader>", "<cr>","r"],
            "commands": ["workbench.action.openRecent"]
        },
        {
            "before": ["<leader>", "g", "c","c"],
            "commands": ["git.commit"]
        },
        {
            "before": ["<leader>", "g", "s"],
            "commands": ["git.stage"]
        },
        {
            "before": ["<leader>", "g", "c","e"],
            "commands": ["git.commitStagedAmend"]
        },
        {
            "before": ["<leader>", "d", "d"],
            "after": ["F5"],
            // "commands": ["workbench.action.debug.run"]
        },
        {
            "before": ["<leader>", "d", "D"],
            "commands": ["workbench.action.debug.run"]
        },
        {
            "before": ["<leader>", "d", "r"],
            "commands": ["workbench.action.debug.restart"]
        },
        {
            "before": ["<leader>", "d", "C"],
            "commands": ["workbench.action.debug.stop"]
        },
        {
            "before": ["<leader>", "d", "c"],
            "commands": ["workbench.action.debug.continue"]
        },
        {
            "before": ["<leader>", "p"],
            "commands": ["workbench.action.debug.pause"]
        },
        {
            "before": ["<leader>", "."],
            "commands": ["workbench.action.debug.stepOver"]
        },
        {
            "before": ["<leader>", ">"],
            "commands": ["workbench.action.debug.stepInto"]
        },
        {
            "before": ["<leader>", "^"],
            "commands": ["workbench.action.debug.stepOut"]
        },
        {
            "before": ["<leader>", "<"],
            "commands": ["workbench.action.debug.stepBack"]
        },
        {
            "before": ["<leader>", "d", "h"],
            "commands": ["workbench.action.debug.runToHere"]
        },
        {
            "before": ["<leader>", "d", "o"],
            "commands": ["workbench.action.debug.showOutput"]
        },
        {
            "before": ["<leader>", "d", "?"],
            "commands": ["workbench.action.debug.showDebugHover"]
        },
        {
            "before": ["<leader>", "d", "i"],
            "commands": ["workbench.action.debug.inspect"]
        },
        {
            "before": ["<leader>", "d", "k"],
            "commands": ["workbench.action.debug.frameUp"]
        },
        {
            "before": ["<leader>", "d", "j"],
            "commands": ["workbench.action.debug.frameDown"]
        },
        {
            "before": ["<leader>", "d", "H"],
            "commands": ["workbench.action.debug.showStoppedLine2"]
        },
        {
            "before": ["<leader>", "b"],
            "commands": ["editor.debug.action.toggleInlineBreakpoint"]
        },
        {
            "before": ["<leader>", "b", "c"],
            "commands": ["editor.debug.action.setBreakpointConditional"]
        },
        {
            "before": ["<leader>", "b", "f"],
            "commands": ["workbench.debug.viewlet.action.addFunctionBreakpointAction"]
        },
        {
            "before": ["<leader>", "b", "C"],
            "commands": ["editor.clearBreakpoints"]
        },

        {
            "before": ["<leader>", "g", "u"],
            "commands": ["git.unstageSelectedRanges"],
        },
        {
            "before": ["ctrl+h"],
            "commands": ["workbench.action.focusLeftGroup"]
        },
        {
            "before": ["ctrl+l"],
            "commands": ["workbench.action.focusRightGroup"]
        },
        {
            "before": ["ctrl+k"],
            "commands": ["workbench.action.focusAboveGroup"]
        },
        {
            "before": ["ctrl+j"],
            "commands": ["workbench.action.focusBelowGroup"]
        },
        {
            "before": ["ctrl+w", "h"],
            "commands": ["workbench.action.splitEditorLeft"]
        },
        {
            "before": ["ctrl+w", "l"],
            "commands": ["workbench.action.splitEditorRight"]
        },
        {
            "before": ["ctrl+w", "k"],
            "commands": ["workbench.action.splitEditorUp"]
        },
        {
            "before": ["ctrl+w", "j"],
            "commands": ["workbench.action.splitEditorDown"]
        },

        {
            "before": ["<leader>", "i", "s"],
            "commands": ["editor.action.triggerParameterHints"],
        },
        {
            "before": ["]", "e"],
            "commands": ["editor.action.marker.nextInFiles"],
        },
        {
            "before": ["[", "e"],
            "commands": ["editor.action.marker.prevInFiles"],
        },
        {
            "before": ["g", "q", "%"],
            "commands": ["editor.action.formatDocument"],
        },
        {
            "before": ["m", "enter"],
            "commands": ["workbench.action.tasks.build"],
        },
        {
            "before": ["leader", "i", "m"],
            "commands": ["workbench.action.tasks.build"],
        },
        {
            "before": ["m", "space"],
            "commands": ["workbench.action.tasks.build"],
        },


    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ";"
            ],
            "commands": [
                "vim.showQuickpickCmdLine"
            ]
        },
        {
            "before": [
                ":"
            ],
            "after": [
                ";"
            ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        },
        {
            "before": [ "\"", "\"" ],
            "after": [ "\"", "*" ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "s"
            ],
            "commands": [
                "leap.find"
            ]
        }
    ],
    "python.defaultInterpreterPath": "/home/william/src/keeb_snakeskin/venv/bin/python",
    "OcpCadViewer.view.center_grid": true,
    "OcpCadViewer.view.grid_XY": true,
    "OcpCadViewer.advanced.autostart": false,
    "git.openRepositoryInParentFolders": "always",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}",
    "window.titleBarStyle": "custom",
    "r.plot.useHttpgd": true,
    "editor.lineHeight": 1.3,
    "cursorless.hatVerticalOffset": -10,
    "cursorless.hatSizeAdjustment": 10,
    "cursorless.hatEnablement.shapes": {
        "bolt": true,
        "ex": true,
        "curve": true
    },
    "cursorless.colors.light": {
        "pink": "#ffb6da",
        "blue": "#0867d3",
        "default": "#5c5965"
    },
    "cursorless.colors.dark": {
        "blue": "#0867d3"
    },
    "remote.autoForwardPortsSource": "hybrid",
    "quarto.visualEditor.lineNumbers": false,

    "workbench.colorCustomizations": {
        "editorError.foreground":   "#ff000068",
        "editorWarning.foreground": "#ffe60043",
        "editorInfo.foreground":    "#00ff0030"
    },
    "editor.suggest.localityBonus": true,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on"
    },
    "editor.tabCompletion": "on",
    "window.customTitleBarVisibility": "auto",
    "window.menuBarVisibility": "visible",
    "git.allowForcePush": true,
    "git.autoStash": true,
    "git.mergeEditor": true,
    "magit.display-buffer-function": "same-column",
    "git.untrackedChanges": "separate",
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 13,
    "github.copilot.enable": {
        "quarto": true
    },
}
