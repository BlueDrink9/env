# vim:ft=sxhkdrc
# vim:ft=sh

# terminal emulator
ctrl + alt + t
	kitty

ctrl + alt + b
  gtk-launch google-chrome.desktop || \
    xdg-open /usr/share/applications/google-chrome-stable || \
    xdg-open ~/usr/share/applications/google-chrome-stable || \
    google-chrome-stable

# ctrl + alt + e
# 	dolphin
super + e
  xdg-open ~ || \
    dolphin

super + v
    gvim

